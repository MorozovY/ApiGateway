{
  "realm": "api-gateway",
  "enabled": true,
  "displayName": "API Gateway",
  "displayNameHtml": "<b>API Gateway</b>",
  "sslRequired": "external",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "permanentLockout": false,
  "maxFailureWaitSeconds": 900,
  "minimumQuickLoginWaitSeconds": 60,
  "waitIncrementSeconds": 60,
  "quickLoginCheckMilliSeconds": 1000,
  "maxDeltaTimeSeconds": 43200,
  "failureFactor": 5,
  "defaultRole": {
    "name": "default-roles-api-gateway",
    "description": "${role_default-roles}",
    "composite": true,
    "clientRole": false
  },
  "roles": {
    "realm": [
      {
        "name": "admin-ui:admin",
        "description": "Полный доступ к Admin UI: управление пользователями, approve/reject routes",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "admin-ui:developer",
        "description": "Разработчик: создание и редактирование routes, просмотр метрик",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "admin-ui:security",
        "description": "Security: approve/reject routes, rollback published routes",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "api:consumer",
        "description": "API Consumer: доступ к protected API routes через gateway",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "default-roles-api-gateway",
        "description": "${role_default-roles}",
        "composite": true,
        "clientRole": false,
        "composites": {
          "realm": []
        }
      }
    ]
  },
  "clients": [
    {
      "clientId": "gateway-admin-ui",
      "name": "Gateway Admin UI",
      "description": "Frontend приложение для администрирования API Gateway",
      "enabled": true,
      "publicClient": true,
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": true,
      "rootUrl": "http://localhost:3000",
      "baseUrl": "/",
      "redirectUris": [
        "http://localhost:3000/*",
        "https://gateway.ymorozov.ru/*"
      ],
      "webOrigins": [
        "http://localhost:3000",
        "https://gateway.ymorozov.ru"
      ],
      "protocol": "openid-connect",
      "attributes": {
        "pkce.code.challenge.method": "S256",
        "post.logout.redirect.uris": "http://localhost:3000/*##https://gateway.ymorozov.ru/*"
      },
      "defaultClientScopes": [
        "web-origins",
        "acr",
        "roles",
        "profile",
        "email"
      ],
      "optionalClientScopes": [
        "address",
        "phone",
        "offline_access",
        "microprofile-jwt"
      ]
    },
    {
      "clientId": "gateway-admin-api",
      "name": "Gateway Admin API",
      "description": "Backend API для администрирования (bearer-only, валидация JWT)",
      "enabled": true,
      "publicClient": false,
      "bearerOnly": true,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": true,
      "protocol": "openid-connect",
      "defaultClientScopes": [
        "web-origins",
        "acr",
        "roles",
        "profile",
        "email"
      ],
      "optionalClientScopes": []
    },
    {
      "clientId": "gateway-core",
      "name": "Gateway Core",
      "description": "API Gateway runtime (bearer-only, валидация consumer JWT)",
      "enabled": true,
      "publicClient": false,
      "bearerOnly": true,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": true,
      "protocol": "openid-connect",
      "defaultClientScopes": [
        "web-origins",
        "acr",
        "roles",
        "profile",
        "email"
      ],
      "optionalClientScopes": []
    },
    {
      "clientId": "company-a",
      "name": "Company A API Consumer",
      "description": "Тестовый API consumer для демонстрации Client Credentials flow",
      "enabled": true,
      "publicClient": false,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": true,
      "protocol": "openid-connect",
      "secret": "company-a-secret-change-in-production",
      "attributes": {
        "use.refresh.tokens": "false"
      },
      "defaultClientScopes": [
        "web-origins",
        "acr",
        "roles",
        "profile",
        "email"
      ],
      "optionalClientScopes": []
    },
    {
      "clientId": "company-b",
      "name": "Company B API Consumer",
      "description": "Тестовый API consumer для E2E тестов (premium tier)",
      "enabled": true,
      "publicClient": false,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": true,
      "protocol": "openid-connect",
      "secret": "company-b-secret-change-in-production",
      "attributes": {
        "use.refresh.tokens": "false"
      },
      "defaultClientScopes": [
        "web-origins",
        "acr",
        "roles",
        "profile",
        "email"
      ],
      "optionalClientScopes": []
    },
    {
      "clientId": "company-c",
      "name": "Company C API Consumer",
      "description": "Тестовый API consumer для E2E rate limit тестов (low tier)",
      "enabled": true,
      "publicClient": false,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": true,
      "protocol": "openid-connect",
      "secret": "company-c-secret-change-in-production",
      "attributes": {
        "use.refresh.tokens": "false"
      },
      "defaultClientScopes": [
        "web-origins",
        "acr",
        "roles",
        "profile",
        "email"
      ],
      "optionalClientScopes": []
    }
  ],
